{"ast":null,"code":"export default {\n  name: 'todo-item',\n  props: {\n    todo: {\n      type: Object,\n      required: true\n    },\n    checkAll: {\n      type: Boolean,\n      required: true\n    }\n  },\n  watch: {\n    checkAll() {\n      if (this.checkAll) {\n        this.completed = true;\n      } else {\n        this.completed = this.todo.completed;\n      }\n    }\n  },\n  data() {\n    return {\n      id: this.todo.id,\n      title: this.todo.title,\n      completed: this.todo.completed,\n      editing: this.todo.editing,\n      lastTitle: ''\n    };\n  },\n  directives: {\n    focus: {\n      inserted: function (el) {\n        el.focus();\n      }\n    }\n  },\n  methods: {\n    removeTodo(id) {\n      this.eventBus.$emit('removeTodo', id);\n    },\n    editTodo() {\n      if (this.completed == true) {\n        this.editing = false;\n      } else {\n        this.lastTitle = this.title;\n        this.editing = true;\n        this.eventBus.$emit('editingTodo', {\n          id: this.id,\n          title: this.title,\n          completed: this.completed,\n          editing: this.editing\n        });\n      }\n    },\n    doneEdit() {\n      if (this.title.trim().length == 0) {\n        this.title = this.lastTitle;\n      }\n      this.editing = false;\n      this.eventBus.$emit('finishEdit', {\n        id: this.id,\n        title: this.title,\n        completed: this.completed,\n        editing: this.editing\n      });\n    },\n    cancelEdit() {\n      this.title = this.lastTitle;\n      this.editing = false;\n    }\n  }\n};","map":{"version":3,"names":["name","props","todo","type","Object","required","checkAll","Boolean","watch","completed","data","id","title","editing","lastTitle","directives","focus","inserted","el","methods","removeTodo","eventBus","$emit","editTodo","doneEdit","trim","length","cancelEdit"],"sources":["src/components/CrudItem.vue"],"sourcesContent":["<template>\n    <div class=\"todo-item\">\n        <div class=\"todo-item-left\">\n            <input v-model=\"completed\" type=\"checkbox\" @change=\"doneEdit\" />\n            <div\n                v-if=\"!editing\"\n                @dblclick=\"editTodo\"\n                class=\"todo-item-label\"\n                :class=\"{ completed: completed }\"\n            >\n                {{ title }}\n            </div>\n            <input\n                v-else\n                class=\"todo-item-edit\"\n                type=\"text\"\n                v-model=\"title\"\n                @blur=\"doneEdit\"\n                @keyup.enter=\"doneEdit\"\n                @keyup.esc=\"cancelEdit\"\n                v-focus\n            />\n        </div>\n        <div class=\"remove-item\" @click=\"removeTodo(todo.id)\">\n            <div class=\"icon-trash\" style=\"float: left\">\n                <div class=\"trash-lid\" style=\"background-color: #2cc3b5\"></div>\n                <div\n                    class=\"trash-container\"\n                    style=\"background-color: #2cc3b5\"\n                ></div>\n                <div class=\"trash-line-1\"></div>\n                <div class=\"trash-line-2\"></div>\n                <div class=\"trash-line-3\"></div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n    name: 'todo-item',\n    props: {\n        todo: {\n            type: Object,\n            required: true,\n        },\n        checkAll: {\n            type: Boolean,\n            required: true,\n        },\n    },\n    watch: {\n        checkAll() {\n            if(this.checkAll) {\n                this.completed =true\n            }\n            else {\n                this.completed = this.todo.completed\n            }\n        }\n    },\n    data() {\n        return {\n            id: this.todo.id,\n            title: this.todo.title,\n            completed: this.todo.completed,\n            editing: this.todo.editing,\n            lastTitle: '',\n        };\n    },\n    directives: {\n        focus: {\n            inserted: function (el) {\n                el.focus();\n            },\n        },\n    },\n    methods: {\n        removeTodo(id) {\n            this.eventBus.$emit('removeTodo', id);\n        },\n        editTodo() {\n            if (this.completed == true) {\n                this.editing = false;\n            } else {\n                this.lastTitle = this.title;\n                this.editing = true;\n                this.eventBus.$emit('editingTodo', {\n                id: this.id,\n                title: this.title,\n                completed: this.completed,\n                editing: this.editing,\n            });\n            }\n        },\n        doneEdit() {\n            if (this.title.trim().length == 0) {\n                this.title = this.lastTitle;\n            }\n            this.editing = false;\n            this.eventBus.$emit('finishEdit', {\n                id: this.id,\n                title: this.title,\n                completed: this.completed,\n                editing: this.editing,\n            });\n        },\n        cancelEdit() {\n            this.title = this.lastTitle;\n            this.editing = false;\n        },\n    },\n};\n</script>\n"],"mappings":"AAuCA;EACAA,IAAA;EACAC,KAAA;IACAC,IAAA;MACAC,IAAA,EAAAC,MAAA;MACAC,QAAA;IACA;IACAC,QAAA;MACAH,IAAA,EAAAI,OAAA;MACAF,QAAA;IACA;EACA;EACAG,KAAA;IACAF,SAAA;MACA,SAAAA,QAAA;QACA,KAAAG,SAAA;MACA,OACA;QACA,KAAAA,SAAA,QAAAP,IAAA,CAAAO,SAAA;MACA;IACA;EACA;EACAC,KAAA;IACA;MACAC,EAAA,OAAAT,IAAA,CAAAS,EAAA;MACAC,KAAA,OAAAV,IAAA,CAAAU,KAAA;MACAH,SAAA,OAAAP,IAAA,CAAAO,SAAA;MACAI,OAAA,OAAAX,IAAA,CAAAW,OAAA;MACAC,SAAA;IACA;EACA;EACAC,UAAA;IACAC,KAAA;MACAC,QAAA,WAAAA,CAAAC,EAAA;QACAA,EAAA,CAAAF,KAAA;MACA;IACA;EACA;EACAG,OAAA;IACAC,WAAAT,EAAA;MACA,KAAAU,QAAA,CAAAC,KAAA,eAAAX,EAAA;IACA;IACAY,SAAA;MACA,SAAAd,SAAA;QACA,KAAAI,OAAA;MACA;QACA,KAAAC,SAAA,QAAAF,KAAA;QACA,KAAAC,OAAA;QACA,KAAAQ,QAAA,CAAAC,KAAA;UACAX,EAAA,OAAAA,EAAA;UACAC,KAAA,OAAAA,KAAA;UACAH,SAAA,OAAAA,SAAA;UACAI,OAAA,OAAAA;QACA;MACA